<p id="notice"><%= notice %></p>

<h1>Groups</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Address</th>
      <th>Photo</th>
      <th>Event start</th>
      <th>Rsvp send before</th>
      <th>Repeats every</th>   
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @groups.each do |group| %>
      <tr>
       
        <td><%= link_to group.title, group %></td>
        <td><%= group.description %></td>
        <td><%= group.address %></td>
        <td><%= group.photo %></td>
        <td><%= group.event_start %></td>
        <td><%= group.rsvp_send_before %></td>
        <td><%= group.repeats_every %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group', new_group_path %>
